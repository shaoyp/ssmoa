<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">

<struts>

	<package name="upload" extends="dreamer" namespace="/upload"> 
		<action name="upload_*" class="org.dreamer.upload.action.UploadAction" method="{1}">
			<result name="show">/WEB-INF/jsp/upload/upload.jsp</result>
			<result name="upload" type="json"></result>
		</action>
	</package>

	
</struts>
